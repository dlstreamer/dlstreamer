{
    "json_schema_version": "2.2.0",
    "input_preproc": [
        {
            "params": {
                "resize": "aspect-ratio",
                "range": [0.0, 1.0]
            }
        }
    ],
    "output_postproc": [
        {
            "converter": "label",
            "method": "softmax",
            "labels": [
                "anger",
                "contempt",
                "disgust",
                "fear",
                "happiness",
                "neutral",
                "sadness",
                "surprise"
            ]
        }
    ]
}
