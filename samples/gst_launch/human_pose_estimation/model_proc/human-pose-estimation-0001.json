                "neck",
                "shoulder_r",
                "elbow_r",
                "wrist_r",
                "shoulder_l",
                "elbow_l",
                "wrist_l",
                "hip_r",
                "knee_r",
                "ankle_r",
                "hip_l",
                "knee_l",
                "ankle_l",
                "eye_r",
                "eye_l",
                "ear_r",
                "ear_l"
            ],
            "point_connections": [
                "shoulder_l",
                "shoulder_r",
                "nose",
                "eye_l",
                "nose",
                "eye_r",
                "eye_l",
                "ear_l",
                "eye_r",
                "ear_r",
                "elbow_l",
                "shoulder_l",
                "elbow_r",
                "shoulder_r",
                "wrist_l",
                "elbow_l",
                "wrist_r",
                "elbow_r",
                "hip_l",
                "knee_l",
                "hip_r",
                "knee_r",
                "knee_l",
                "ankle_l",
                "knee_r",
                "ankle_r"
            ]
        }
    ]
}
