{
    "json_schema_version": "2.1.0",
    "input_preproc": [
        {
            "format": "image",
            "layer_name": "data",
            "params": {
                "color_space": "BGR",
                "resize": "aspect-ratio",
                "padding": {
                    "stride": 8,
                    "fill_value": [0.0, 0.0, 0.0]
                }
            }
        }
    ],
